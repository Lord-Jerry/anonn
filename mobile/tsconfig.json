{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@constant/*": ["./src/constant/*"],
      "@assets/*": ["./assets/*"],
    }
  },
  "exclude": ["node_modules"]
  // "include": ["src", "components"]
}
